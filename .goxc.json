{
	"AppName": "goxc",
	"ArtifactsDest": "../goxc-pages/dl",
	"Tasks": [
		"interpolate-source",
		"go-fmt",
		"default",
		"deb-source"
	],
	"ResourcesInclude": "README.md,.\\_test1/*,_test2",
	"ResourcesExclude": "*.go",
	"MainDirsExclude": "Godeps",
	"PackageVersion": "0.14.1",
	"TaskSettings": {
		"archive-zip": {},
		"bintray": {
			"downloadspage": "bintray.md",
			"package": "goxc",
			"repository": "utils",
			"subject": "laher"
		},
		"codesign": {
			"id": "goxc"
		},
		"deb-gen": {
			"metadata": {
				"description": "Cross-compiler utility for Go",
				"maintainer": "Am Laher (https://github.com/laher)"
			},
			"metadata-deb": {
				"Depends": "golang",
				"Homepage": "https://github.com/laher/goxc"
			}
		},
		"downloads-page": {
			"filename": "index.md",
			"templateFile": "downloads.tpl"
		},
		"pkg-build": {
			"metadata": {
				"description": "Cross-compiler utility for Go",
				"maintainer": "Am Laher (https://github.com/laher)"
			},
			"metadata-deb": {
				"Depends": "golang",
				"Homepage": "https://github.com/laher/goxc"
			}
		},
		"xc": {
			"test-setting": "test-value"
		}
	},
	"ConfigVersion": "0.9",
	"BuildSettings": {
		"LdFlags": "-s"
	},
	"Env": [
		"GOPATH=Godeps{{.PLS}}{{.Env.GOPATH}}"
	]
}
